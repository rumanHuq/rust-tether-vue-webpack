{
  "extends": "tslint:recommended",
  "rules": {
    "ordered-imports": false,
    "trailing-comma": true,
    "no-object-literal-type-assertion": false,
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    }
  },
  "jsRules": {}
}
