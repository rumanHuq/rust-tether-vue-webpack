  "scripts": {
    "storybook": "kill-port 9001 && start-storybook -p 9001 -c .storybook",
    "prod": "rimraf ../src/statc/* && cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "analyze": "rimraf ../src/statc/* && cross-env NODE_ENV=production SHOW_BUNDLE=true TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "start": "rimraf ../src/statc/* && cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand",
    "prettier": "prettier --trailing-comma all --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,ts,json,vue}\"",
    "stylelint": "stylelint '**/*.scss' --syntax scss && stylelint '**/*.vue' --syntax scss --config .vuestylelintrc",
    "tslint": "tslint --project tsconfig.json --fix",
    "test": "jest",
    "lint:all": "yarn prettier && yarn stylelint && yarn tslint",
    "precommit": "lint-staged && yarn test"
  },
  "husky": {
